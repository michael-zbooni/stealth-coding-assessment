{
  "name": "stealth-coding-assessment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm install && npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src src/index.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint --ext .ts src",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts -p",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "@emailjs/nodejs": "^2.2.0",
    "@jmondi/oauth2-server": "^2.6.1",
    "@types/pg": "^8.6.6",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "pg": "^8.11.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.16",
    "typeorm-extension": "^2.8.0",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.58",
    "@swc/jest": "^0.2.26",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "commitizen": "^4.3.0",
    "commitizen-emoji": "^1.0.5",
    "commitlint": "^17.6.3",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "typescript": "^5.0.4"
  }
}
